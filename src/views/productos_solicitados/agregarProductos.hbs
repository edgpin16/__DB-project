<div class="container-contact100">
	<div class="wrap-contact100">
        <span class="contact100-form-title">
		Selecciona un producto:
		</span>
        {{#each elementosPrueba}}
		<form class="contact100-form validate-form" action="/productos_solicitados/modificarProducto" method="GET">

			<div class="wrap-input100 input100-select" style="margin-top: 20px;">
				<span class="label-input100"></span>
				<div class="form-check">
					<div>
						<input class="form-check-input" type="radio" name={{nombre}} id="ID" value={{id}}>
						<label class="form-check-label" for="ID">
                            Nombre: {{nombre}} <br>
                            Cantidad: {{cantidad}} <br>
							Costo: {{costo}} <br>
						</label>
					</div>
				</div>
			</div>

			<div class="container-contact100-form-btn">
				<div class="wrap-contact100-form-btn">
					<div class="contact100-form-bgbtn"></div>
					<button class="contact100-form-btn">
						<span>
							Agregar
							<i class="fa fa-long-arrow-right m-l-7" aria-hidden="true"></i>
						</span>
					</button>
				</div>
			</div>
		</form>
        {{/each}}
	</div>
</div>