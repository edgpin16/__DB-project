<div class="container-contact100">
	<div class="wrap-contact100">
		<form class="contact100-form validate-form" action="/productos_solicitados/agregarProductos" method="GET">
			<span class="contact100-form-title">
				Agregar productos para los pedidos
			</span>

			<div class="wrap-input100 input100-select">
				<span class="label-input100">Selecciona un pedido</span>
				<div class="form-check">
					{{#each masterObject}}
					<div>
						<input class="form-check-input" type="radio" name="ID_Pedido" id="ID" value={{ID}}>
						<label class="form-check-label" for="ID">
							Farmacia: {{nombreFarmacia}} {{ciudadFarmacia}} <br> analista: {{analista}} <br> Laboratorio: {{nombreLaboratorio}} {{ciudadLaboratorio}}
						</label>
					</div>
					{{/each}}
				</div>
			</div>

			<div class="container-contact100-form-btn">
				<div class="wrap-contact100-form-btn">
					<div class="contact100-form-bgbtn"></div>
					<button class="contact100-form-btn">
						<span>
							Submit
							<i class="fa fa-long-arrow-right m-l-7" aria-hidden="true"></i>
						</span>
					</button>
				</div>
			</div>
		</form>
	</div>
</div>